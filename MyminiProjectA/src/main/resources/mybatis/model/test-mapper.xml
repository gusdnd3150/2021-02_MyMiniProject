<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="user">
	
	<select id="selectUser" resultType="userVo" parameterType="userVo">
		SELECT *
		 FROM users
		 where userId=#{userId}
	</select>
	
	<select id="all" resultType="map">
	SELECT * FROM boards
	</select>
	
	<insert id="userJoin" parameterType="userVo" useGeneratedKeys="true" keyProperty="id">
      INSERT INTO users (userId,userName,userEmail,userPassword,autho,joinDate)
      VALUES (#{userId}, #{userName}, #{userEmail}, #{userPassword}, #{autho}, NOW())
	</insert>

</mapper>